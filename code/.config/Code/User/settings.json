{
  // Editor
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.cursorBlinking": "smooth",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.fontFamily": "'FiraCode Nerd Font', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13.5,
  "editor.lineNumbers": "relative",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "gutter",
  "editor.rulers": [
    80,
    120
  ],
  "editor.semanticHighlighting.enabled": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 80,
  "editor.unicodeHighlight.invisibleCharacters": false,
  // Explorer
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  // Files
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 500,
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/*.o": true,
    "**.o": true,
    "**/*.out": true,
    "**.out": true,
    "**/*.class": true,
    "**.class": true,
    "**.vscode": true,
  },
  "files.insertFinalNewline": false,
  "files.trimFinalNewlines": false,
  "files.associations": {
    ".sequelizerc": "javascript",
    ".stylelintrc": "json",
    "*.tsx": "typescriptreact",
    ".env.*": "dotenv",
    ".prettierrc": "json"
  },
  "workbench.editor.labelFormat": "short",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  // Window
  "window.menuBarVisibility": "toggle",
  // Extension
  "extensions.ignoreRecommendations": true,
  // Terminal
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell",
    },
  },
  "terminal.integrated.allowChords": false,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono",
  "terminal.integrated.splitCwd": "workspaceRoot",
  "terminal.external.linuxExec": "kitty",
  "terminal.external.windowsExec": "pwsh",
  // Security
  "security.workspace.trust.enabled": false,
  // Language
  // Rules
  "[java]": {
    "editor.tabSize": 4,
  },
  "[json]": {
    "editor.formatOnSave": false 
  },
  "[c]": {
    "editor.tabSize": 4,
    "files.encoding": "windows1252",
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
  },
  "emmet.syntaxProfiles": {
    "javascript": "tsx"
  },
  // Options 
  // js
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  // ts
  "typescript.tsserver.log": "off",
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  // Multicommands
  "multiCommand.commands": [
    {
      "command": "multiCommand.searchReplace",
      "sequence": [
          "actions.find",
          "editor.action.startFindReplaceAction",
          "actions.find"
      ]
  },
  ],
  // Vim
  "vim.easymotion": true,
  "vim.enableNeovim": true,
  "vim.foldfix": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.incsearch": true,
  "vim.leader": "<space>",
  "vim.hlsearch": true,
  "vim.sneak": true,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.highlightedyank.enable": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", ";"],
      "commands": ["workbench.action.showCommands"],
      "silent": true
    },
    // Buffers 
    {
      "before": ["L"],
      "commands": ["workbench.action.nextEditor"],
      "silent": true
    },
    {
      "before": ["H"],
      "commands": ["workbench.action.previousEditor"],
      "silent": true
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.save"],
      "silent": true
    },
    {
      "before": ["<leader>", "v", "s"],
      "commands": ["workbench.action.splitEditor"],
      "silent": true
    },
    {
      "before": ["<leader>", "v", "h"],
      "commands": ["workbench.action.splitEditorOrthogonal"],
      "silent": true
    },
    {
      "before": ["<leader>", "/"],
      "commands": ["editor.action.commentLine"],
      "silent": true
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["workbench.action.closeActiveEditor"],
      "silent": true
    },
    {
      "before": ["<leader>", "C"],
      "commands": ["workbench.action.closeAllEditors"],
      "silent": true
    },
    {
      "before": ["<leader>", "q"],
      "commands": ["workbench.action.closeWindow"],
      "silent": true
    },
    // Search
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"],
      "silent": true
    },
    {
      "before": ["<leader>","f", "t"],
      "commands": ["workbench.action.replaceInFiles"],
      "silent": true
    },
    {
      "before": ["<leader>", "s", "r"],
      "commands": ["multiCommand.searchReplace"],
      "silent": true
    },
    // LSP
    {
      "before": ["K"],
      "commands": ["editor.action.showHover"],
      "silent": true
    },
    {
      "before": ["g", "d"],
      "commands": ["editor.action.revealDefinition"],
      "silent": true
    },
    {
      "before": ["g", "D"],
      "commands": ["editor.action.revealDeclaration"],
      "silent": true
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.goToReferences"],
      "silent": true
    },
    {
      "before": ["g", "I"],
      "commands": ["editor.action.goToImplementation"],
      "silent": true
    },
    {
      "before": ["g", "s"],
      "commands": ["editor.action.triggerParameterHints"],
      "silent": true
    },
    {
      "before": ["g", "l"],
      "commands": ["editor.action.marker.next"],
      "silent": true
    },
    {
      "before": ["<leader>", "l", "a"],
      "commands": ["editor.action.quickFix"],
      "silent": true
    },
    {
      "before": ["<leader>", "l", "f"],
      "commands": ["editor.action.format"],
      "silent": true
    },
    {
      "before": ["<leader>", "l", "q"],
      "commands": ["editor.action.autoFix"],
      "silent": true
    },
    {
      "before": ["<leader>", "l", "w"],
      "commands": ["workbench.action.problems.focus"],
      "silent": true
    },
    {
      "before": ["<leader>", "l", "r"],
      "commands": ["editor.action.rename"],
      "silent": true
    },
    {
      "before": ["<leader>", "l", "s"],
      "commands": ["workbench.action.gotoSymbol"],
      "silent": true
    },
    {
      "before": ["<leader>", "l", "S"],
      "commands": ["workbench.action.showAllSymbols"],
      "silent": true
    },
    // Debug
    {
      "before": ["<leader>", "d", "t"],
      "commands": ["editor.debug.action.toggleBreakpoint"],
      "silent": true
    },
    {
      "before": ["<leader>", "d", "b"],
      "commands": ["workbench.action.debug.stepBack"],
      "silent": true
    },
    {
      "before": ["<leader>", "d", "c"],
      "commands": ["workbench.action.debug.continue"],
      "silent": true
    },
    {
      "before": ["<leader>", "d", "C"],
      "commands": ["editor.debug.action.runToCursor"],
      "silent": true
    },
    {
      "before": ["<leader>", "d", "d"],
      "commands": ["workbench.action.debug.disconnect"],
      "silent": true
    },
    {
      "before": ["<leader>", "d", "i"],
      "commands": ["workbench.action.debug.stepInto"],
      "silent": true
    },
    {
      "before": ["<leader>", "d", "o"],
      "commands": ["workbench.action.debug.stepOver"],
      "silent": true
    },
    {
      "before": ["<leader>", "d", "u"],
      "commands": ["workbench.action.debug.stepOut"],
      "silent": true
    },
    {
      "before": ["<leader>", "d", "p"],
      "commands": ["workbench.action.debug.pause"],
      "silent": true
    },
    {
      "before": ["<leader>", "d", "r"],
      "commands": ["workbench.debug.action.toggleRepl"],
      "silent": true
    },
    {
      "before": ["<leader>", "d", "s"],
      "commands": ["workbench.action.debug.start"],
      "silent": true
    },
    {
      "before": ["<leader>", "d", "q"],
      "commands": ["workbench.action.debug.stop"],
      "silent": true
    },
    // better db
    {
      "before": ["d", "b"],
      "after": ["v", "b", "\"", "_", "d"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": [":nohl"],
      "silent": true
    },
    {
      "before": ["<leader>", "n"],
      "commands": ["extension.advancedNewFile"],
      "silent": true
    },
    {
      "before": ["<leader>", "g", "g"],
      "commands": ["workbench.action.terminal.openNativeConsole"],
      "silent": true
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<"],
      "after": ["<", "g", "v"],
    },
    {
      "before": [">"],
      "after": [">", "g", "v"],
    },
    {
      "before": ["<leader>", "s", "r"],
      "commands": ["vim-search-and-replace.start"],
      "silent": true
    },
    {
      "before": ["<leader>", "/"],
      "commands": ["editor.action.blockComment"],
      "silent": true
    },
    {
      "before": ["<leader>", "l", "f"],
      "commands": ["editor.action.formatSelection"],
      "silent": true
    },
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["k", "j"],
      "after": ["<Esc>"]
    },
  ],
  "vim.handleKeys": {
    "<C-b>": false,
    "<C-e>": false,
    "<C-f>": false,
    "<C-z>": false,
    "<C-y>": false,
    "<C-t>": false,
  },
  // End Vim
  // Extensions 
  // css rem
  "cssrem.hover": "always",
  "cssrem.vw": true,
  "cssrem.fixedDigits": 3,
  "cssrem.rootFontSize": 24,
  // Material Theme
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.folders.associations": {
    "infra": "app",
    "entities": "class",
    "domain": "class",
    "schemas": "class",
    "typeorm": "database",
    "repositories": "mappings",
    "http": "container",
    "migrations": "tools",
    "modules": "components",
    "implementations": "core",
    "dtos": "typescript",
    "fakes": "mock",
    "websockets": "pipe",
    "protos": "pipe",
    "grpc": "pipe",
    "providers": "include",
    "subscribers": "messages",
    "useCases": "controller",
    "kafka": "scripts",
    "mappers": "meta",
    "_shared": "shared",
    "eslint-config": "tools",
    "kube": "kubernetes"
  },
  "material-icon-theme.languages.associations": {
    "dotenv": "tune"
  },
  "material-icon-theme.files.associations": {
    "ormconfig.json": "database",
    "tsconfig.json": "tune",
    "*.proto": "3d",
    "*.webpack.js": "webpack"
  },
  "workbench.productIconTheme": "fluent-icons",
  // Advanced new file
  "advancedNewFile.exclude": {
    "node_modules": true,
    "dist": true,
    "dev": true,
    ".vscode": true,
    ".git": true
  },
  "advancedNewFile.convenienceOptions": [
    "last",
    "current",
    "root"
  ],
  "explorer.confirmDragAndDrop": false,
  "workbench.colorTheme": "Tokyo Night",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "editor.tokenColorCustomizations": {
    "[Tokyo Night]": { // or "[Tokyo Night Storm]"
      "textMateRules": [
        {
          "scope": [
            "comment",
            "meta.var.expr storage.type",
            "keyword.control.flow",
            "keyword.control.return",
            "meta.directive.vue punctuation.separator.key-value.html",
            "meta.directive.vue entity.other.attribute-name.html",
            "tag.decorator.js entity.name.tag.js",
            "tag.decorator.js punctuation.definition.tag.js",
            "storage.modifier"
          ],
          "settings": {
            "fontStyle": ""
          }
        }
      ]
    }
  },
  "breadcrumbs.enabled": false,
  "Lua.telemetry.enable": true,
  "markdown-preview-enhanced.previewTheme": "github-dark.css",
}
